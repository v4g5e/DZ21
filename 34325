    // Задание 98
    class SessionManager
    {
        private static SessionManager _instance;
        private Dictionary<string, object> _sessions = new Dictionary<string, object>();

        public static SessionManager Instance
        {
            get { return _instance ?? (_instance = new SessionManager()); }
        }
        public void CreateSession(string sessionId) => _sessions[sessionId] = new object();
        private SessionManager() { }
    }

    // Задание 99
    class NotificationService
    {
        private static NotificationService _instance;
        public static NotificationService Instance
        {
            get { return _instance ?? (_instance = new NotificationService()); }
        }
        public void Notify(string message) => Console.WriteLine($"Уведомление: {message}");
        private NotificationService() { }
    }

    // Задание 100
    class ResourcePoolManager
    {
        private static ResourcePoolManager _instance;
        private Queue<object> _pool = new Queue<object>();

        public static ResourcePoolManager Instance
        {
            get { return _instance ?? (_instance = new ResourcePoolManager()); }
        }
        public void AddResource(object resource) => _pool.Enqueue(resource);
        public object GetResource() => _pool.Count > 0 ? _pool.Dequeue() : null;
        private ResourcePoolManager() { }
    }

