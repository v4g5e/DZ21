    // Задание 41 - Расширяющий метод для проверки палиндрома
    static class StringExtensions
    {
        public static bool IsPalindrome(this string str)
        {
            string clean = new string(str.Where(char.IsLetterOrDigit).ToArray()).ToLower();
            return clean == new string(clean.Reverse().ToArray());
        }

        // Задание 45 - Удаление пробелов
        public static string RemoveAllSpaces(this string str) => str.Replace(" ", "");

        // Задание 47 - Title Case
        public static string ToTitleCase(this string str) => System.Globalization.CultureInfo.CurrentCulture.TextInfo.ToTitleCase(str);

        // Задание 50 - Подсчет слов
        public static int CountWords(this string str) => str.Split(new[] { ' ' }, StringSplitOptions.RemoveEmptyEntries).Length;

        // Задание 53 - Удаление HTML тегов
        public static string RemoveHtmlTags(this string html)
        {
            System.Text.RegularExpressions.Regex tag = new System.Text.RegularExpressions.Regex("<[^>]+>");
            return tag.Replace(html, "");
        }

        // Задание 56 - Base64 кодирование
        public static string ToBase64(this string str) => Convert.ToBase64String(Encoding.UTF8.GetBytes(str));

        // Задание 59 - Проверка силы пароля
        public static int PasswordStrength(this string password)
        {
            int strength = 0;
            if (password.Length >= 8) strength++;
            if (password.Any(char.IsUpper)) strength++;
            if (password.Any(char.IsLower)) strength++;
            if (password.Any(char.IsDigit)) strength++;
            return strength;
        }
    }

    // Задание 42 - Случайный элемент массива
    static class ArrayExtensions
    {
        public static T GetRandomElement<T>(this T[] array)
        {
            return array[new Random().Next(array.Length)];
        }

        // Задание 51 - Медиана массива
        public static double GetMedian(this int[] array)
        {
            var sorted = array.OrderBy(x => x).ToArray();
            return sorted.Length % 2 == 0 ? 
                (sorted[sorted.Length / 2 - 1] + sorted[sorted.Length / 2]) / 2.0 : 
                sorted[sorted.Length / 2];
        }

        // Задание 57 - Второй максимальный элемент
        public static int GetSecondMax(this int[] array) => array.OrderByDescending(x => x).Skip(1).First();
    }

    // Задание 43 - Проверка четности
    static class NumberExtensions
    {
        public static bool IsEven(this int number) => number % 2 == 0;

        // Задание 48 - Римские цифры
        public static string ToRoman(this int number)
        {
            string result = "";
            int[] values = { 1000, 900, 500, 400, 100, 90, 50, 40, 10, 9, 5, 4, 1 };
            string[] numerals = { "M", "CM", "D", "CD", "C", "XC", "L", "XL", "X", "IX", "V", "IV", "I" };

            for (int i = 0; i < values.Length; i++)
            {
                while (number >= values[i])
                {
                    result += numerals[i];
                    number -= values[i];
                }
            }
            return result;
        }

        // Задание 54 - Проверка простого числа
        public static bool IsPrime(this int number)
        {
            if (number < 2) return false;
            for (int i = 2; i <= Math.Sqrt(number); i++)
                if (number % i == 0) return false;
            return true;
        }

        // Задание 60 - Читаемый формат байтов
        public static string BytesToReadable(this long bytes)
        {
            string[] sizes = { "B", "KB", "MB", "GB", "TB" };
            double len = bytes;
            int order = 0;
            while (len >= 1024 && order < sizes.Length - 1)
            {
                order++;
                len = len / 1024;
            }
            return $"{len:0.##} {sizes[order]}";
        }
    }

    // Задание 44 - Возраст из DateTime
    static class DateTimeExtensions
    {
        public static int GetAge(this DateTime birthDate)
        {
            int age = DateTime.Now.Year - birthDate.Year;
            if (birthDate.Date > DateTime.Now.AddYears(-age)) age--;
            return age;
        }

        // Задание 52 - Начало недели
        public static DateTime GetWeekStart(this DateTime date)
        {
            int diff = (7 + (date.DayOfWeek - DayOfWeek.Monday)) % 7;
            return date.AddDays(-1 * diff).Date;
        }

        // Задание 58 - Читаемый формат даты
        public static string ToReadableFormat(this DateTime date) => date.ToString("dddd, d MMMM yyyy");
    }

    // Задание 46 - Перемешивание списка
    static class ListExtensions
    {
        public static List<T> Shuffle<T>(this List<T> list)
        {
            Random rng = new Random();
            int n = list.Count;
            while (n > 1)
            {
                n--;
                int k = rng.Next(n + 1);
                T value = list[k];
                list[k] = list[n];
                list[n] = value;
            }
            return list;
        }

        // Задание 49 - Проверка пустоты коллекции
        public static bool HasElements<T>(this List<T> list) => list != null && list.Count > 0;

        // Задание 55 - Группировка по размеру
        public static List<List<T>> GroupBySize<T>(this List<T> list, int size)
        {
            var result = new List<List<T>>();
            for (int i = 0; i < list.Count; i += size)
                result.Add(list.Skip(i).Take(size).ToList());
            return result;
        }
    }

