    // Задание 88 - Исправлен Generic Singleton - без ограничения new()
    class GenericSingleton<T>
    {
        private static GenericSingleton<T> _instance;
        public static GenericSingleton<T> Instance
        {
            get
            {
                if (_instance == null)
                    _instance = new GenericSingleton<T>();
                return _instance;
            }
        }
        public T Value { get; set; }
    }

    // Задание 89
    class DestroyableSingleton
    {
        private static DestroyableSingleton _instance;
        public static DestroyableSingleton Instance
        {
            get { return _instance ?? (_instance = new DestroyableSingleton()); }
        }
        public void Destroy() => _instance = null;
        private DestroyableSingleton() { }
    }

    // Задание 90
