                if (!int.TryParse(choice, out int taskNum) || taskNum < 0 || taskNum > 100)
                {
                    if (taskNum == 0) break;
                    Console.WriteLine("Неверный ввод");
                    continue;
                }

                switch (taskNum)
                {
                    case 0:
                        return;

                    // Задание 1
                    case 1:
                        Counter.count = 0;
