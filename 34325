    class EmailService
    {
        private static EmailService _instance;
        private Queue<string> _messageQueue = new Queue<string>();

        public static EmailService Instance
        {
            get { return _instance ?? (_instance = new EmailService()); }
        }
        public void EnqueueMessage(string message) => _messageQueue.Enqueue(message);
        public string DequeueMessage() => _messageQueue.Count > 0 ? _messageQueue.Dequeue() : "";
        private EmailService() { }
    }

