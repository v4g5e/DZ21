    // Задание 19
    static class TimerHelper
    {
        public static long MeasureTime(Action action)
        {
            var sw = System.Diagnostics.Stopwatch.StartNew();
            action();
            sw.Stop();
            return sw.ElapsedMilliseconds;
        }
    }

